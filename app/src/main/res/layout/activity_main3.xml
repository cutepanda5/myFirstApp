<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:orientation="vertical"
    android:background="#FFFED4">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="#F7EF13">
        <Button
            android:id="@+id/backx3"
            android:layout_width="40dp"
            android:layout_marginLeft="10dp"
            android:layout_height="wrap_content"
            android:background="@null"
            android:layout_gravity="center_vertical"
            android:drawableStart="@drawable/ic_baseline_arrow_back_ios_24" />

        <TextView
            android:id="@+id/head_one"
            android:layout_marginRight="40dp"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="@string/个人报告"
            android:textColor="#000000"
            android:textSize="25sp" />
    </LinearLayout>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="160dp">

                <LinearLayout
                    android:layout_marginTop="30dp"
                    android:layout_width="350dp"
                    android:layout_height="150dp"
                    android:layout_centerInParent="true"
                    android:background="@drawable/background1"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/BiaoQing"
                        android:layout_width="134dp"
                        android:layout_height="match_parent"
                        android:src="@drawable/ceshi"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/zPingYu"
                        android:textColor="#000000"
                        android:gravity="center_vertical"
                        android:textSize="18sp"/>

                </LinearLayout>
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="230dp">

                <LinearLayout
                    android:layout_width="350dp"
                    android:layout_height="190dp"
                    android:layout_centerInParent="true"
                    android:background="@drawable/background1"
                    android:orientation="vertical">

                    <TextView
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="5dp"
                        android:id="@+id/wdxFenXi"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:textColor="#000000"
                        android:textSize="17sp" />

                </LinearLayout>


            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <LinearLayout
                    android:layout_width="350dp"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:background="@drawable/background1"
                    android:orientation="vertical">
                    <TextView
                        android:gravity="center_vertical"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:drawableStart="@drawable/ic_baseline_menu_book_24"
                        android:layout_marginLeft="10dp"
                        android:id="@+id/two1"
                        android:textSize="20sp"
                        android:textColor="#36BAF5">

                    </TextView>
                    <TextView
                        android:layout_marginLeft="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:layout_weight="3"
                        android:id="@+id/two2"
                        android:textSize="18sp"
                        android:textColor="#000000"
                        android:gravity="center_vertical"/>

                    <TextView
                        android:gravity="center_vertical"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:drawableStart="@drawable/ic_baseline_menu_book_24"
                        android:layout_marginLeft="10dp"
                        android:id="@+id/three1"
                        android:textSize="20sp"
                        android:textColor="#36BAF5">

                    </TextView>
                    <TextView
                        android:layout_marginLeft="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:layout_weight="3"
                        android:id="@+id/three2"
                        android:textSize="18sp"
                        android:textColor="#000000"
                        android:gravity="center_vertical"/>

                    <TextView
                        android:gravity="center_vertical"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:drawableStart="@drawable/ic_baseline_menu_book_24"
                        android:layout_marginLeft="10dp"
                        android:id="@+id/four1"
                        android:textSize="20sp"
                        android:textColor="#36BAF5">

                    </TextView>
                    <TextView
                        android:layout_marginLeft="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:layout_weight="3"
                        android:id="@+id/four2"
                        android:textSize="18sp"
                        android:textColor="#000000"
                        android:gravity="center_vertical"/>

                    <TextView
                        android:gravity="center_vertical"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:drawableStart="@drawable/ic_baseline_menu_book_24"
                        android:layout_marginLeft="10dp"
                        android:id="@+id/five1"
                        android:textSize="20sp"
                        android:textColor="#36BAF5">

                    </TextView>
                    <TextView
                        android:layout_marginLeft="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:layout_weight="3"
                        android:id="@+id/five2"
                        android:textSize="18sp"
                        android:textColor="#000000"
                        android:gravity="center_vertical">
                    </TextView>


                </LinearLayout>


            </RelativeLayout>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="50dp"/>
        </LinearLayout>
    </ScrollView>
</LinearLayout>
